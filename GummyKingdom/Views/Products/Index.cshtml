@model IEnumerable<GummyKingdom.Models.Product>

<html>
<head>
	<title>Gummy Kingdom</title>
</head>
<body>
	<h2>Gummy Bear Kingdom</h2>
	<h4>@Html.DisplayNameFor(model => model.Name)</h4>
	<ul>
		@foreach (var listProduct in Model)
		{
			<li>@Html.DisplayFor(modelItem => listProduct.Name)</li>
		}
	</ul>
</body>
</html>